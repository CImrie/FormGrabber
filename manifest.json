{
  "manifest_version": 2,

  "name": "Form Grabber",
  "description": "A simple interface to bulk copy/paste form details.",
  "version": "2.0.0",
  "key":"",
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "icons": {
    "32": "assets/img/icon-32.png",
    "48": "assets/img/icon-48.png",
    "128": "assets/img/icon-128.png"
  },

  "browser_action": {
    "default_icon": "assets/img/icon-19.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "background",
    "identity",
    "identity.email",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*"
  ],
  "oauth2": {
    "client_id": "513090491024-s8atoku95h5e7plmtqqfldeuq9ovv0na.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/plus.login",
      "https://www.googleapis.com/auth/chromewebstore.readonly"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "assets/js/require.js",
        "assets/js/jquery-2.1.4.min.js",
        "assets/js/bootbox.min.js",
        "assets/js/moment.js",
        "assets/js/classes/Message.js",
        "assets/js/classes/Site.js",
        "assets/js/classes/SiteManager.js",
        "assets/js/crawler.js"
      ],
      "css": [
        "assets/css/style.css",
        "assets/css/font-awesome.min.css"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "https://apis.google.com/js/client.js",
    "assets/js/require.js",
    "assets/css/bootstrap.min.css",
    "assets/css/font-awesome.min.css",
    "assets/js/jquery-2.1.4.min.js",
    "assets/fonts/*",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*"
  ],
  "background": {
    "scripts": [
      "assets/js/moment.js",
      "assets/js/classes/User.js",
      "assets/js/classes/LicenseManager.js",
      "assets/js/classes/Message.js",
      "assets/js/classes/Site.js",
      "assets/js/classes/SiteManager.js",
      "assets/js/jquery-2.1.4.min.js",
      "assets/css/font-awesome.min.css",
      "assets/js/background.js"
    ]
  }
}
