{
  "manifest_version": 2,

  "name": "Form Grabber",
  "description": "A simple interface to bulk copy/paste form details.",
  "version": "2.0.0",
  "key":"",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "32": "assets/img/icon-32.png",
    "48": "assets/img/icon-48.png",
    "128": "assets/img/icon-128.png"
  },

  "browser_action": {
    "default_icon": "assets/img/icon-19.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "background",
    "identity",
    "identity.email",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*"
  ],
  "oauth2": {
    "client_id": "513090491024-s8atoku95h5e7plmtqqfldeuq9ovv0na.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/plus.login",
      "https://www.googleapis.com/auth/chromewebstore.readonly"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "public/js/page-compiled.js"
      ],
      "css": [
        "public/css/page.css"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    "*"
  ],
  "background": {
    "scripts": [
      "public/js/background.js"
    ]
  }
}
